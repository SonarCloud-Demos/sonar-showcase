{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": ["src"],
  "entryPointStrategy": "expand",
  "out": "target/site/typedoc",
  "exclude": ["**/*.test.ts", "**/*.test.tsx", "**/setupTests.ts", "**/vite-env.d.ts"],
  "excludePrivate": true,
  "excludeProtected": false,
  "excludeInternal": false,
  "readme": "none",
  "skipErrorChecking": false,
  "validation": {
    "notExported": false,
    "invalidLink": true,
    "notDocumented": false
  },
  "name": "SonarShowcase Frontend API",
  "includeVersion": true,
  "categorizeByGroup": true,
  "categoryOrder": [
    "Components",
    "Services",
    "Hooks",
    "Utils",
    "Types"
  ],
  "kindSortOrder": [
    "Project",
    "Module",
    "Namespace",
    "Enum",
    "Class",
    "Interface",
    "TypeAlias",
    "Constructor",
    "Property",
    "Method",
    "Function",
    "Accessor",
    "Variable"
  ]
}

